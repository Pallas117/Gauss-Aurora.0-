VITE_GAUSS_RAG_URL=http://localhost:3002
VITE_HELIO_PROXY_URL=http://localhost:3001
VITE_HELIO_WS_ENABLED=false

# Backend proxy
PROXY_PORT=3001
PROXY_HOST=127.0.0.1
ALLOWED_ORIGINS=http://localhost:8080
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_ANON_KEY=your-anon-key
AUTH_REQUIRED=true
CYBERTIGER_ENABLED=true
CYBERTIGER_RATE_LIMIT_WINDOW_MS=60000
CYBERTIGER_RATE_LIMIT_MAX=120
CYBERTIGER_AUTH_FAILURE_WINDOW_MS=300000
CYBERTIGER_AUTH_FAILURE_THRESHOLD=8
CYBERTIGER_AUTO_BLOCK_SECONDS=900
CYBERTIGER_MAX_EVENTS=5000
LOCAL_INFER_URL=http://localhost:8000
